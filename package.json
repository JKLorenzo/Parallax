{
  "name": "parallax",
  "version": "1.0.0",
  "description": "A multi-purpose Discord bot for gaming.",
  "main": "build/main.js",
  "type": "module",
  "scripts": {
    "start": "node .",
    "clean": "rimraf build",
    "build": "tsc",
    "postbuild": "copyfiles -u 1 \"src/assets/**/*.*\" build",
    "prepare": "is-ci || husky install",
    "forever-start": "forever start ./build/main.js",
    "forever-stop": "forever stop ./build/main.js"
  },
  "engines": {
    "node": ">=22.x"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/JKLorenzo/Parallax.git"
  },
  "keywords": [
    "nodejs",
    "typescript",
    "discord-bot"
  ],
  "author": "Juruel Keanu Lorenzo <keanulorenzo32@gmail.com>",
  "license": "AGPL-3.0-only",
  "bugs": {
    "url": "https://github.com/JKLorenzo/Parallax/issues"
  },
  "homepage": "https://github.com/JKLorenzo/Parallax#readme",
  "dependencies": {
    "async-g-i-s": "^1.5.5",
    "copyfiles": "^2.4.1",
    "discord.js": "^14.19.1",
    "dotenv": "^16.5.0",
    "humanize-duration": "^3.32.1",
    "husky": "^9.1.7",
    "is-ci": "^4.1.0",
    "lodash": "^4.17.21",
    "mongodb": "^6.16.0",
    "rimraf": "^6.0.1"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.8.0",
    "@commitlint/config-conventional": "^19.8.0",
    "@types/humanize-duration": "^3.27.4",
    "@types/lodash": "^4.17.16",
    "@typescript-eslint/eslint-plugin": "^8.31.0",
    "@typescript-eslint/parser": "^8.31.0",
    "eslint": "^9.25.1",
    "eslint-config-prettier": "^10.1.2",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-prettier": "^5.2.6",
    "forever": "^4.0.3",
    "lint-staged": "^15.5.1",
    "prettier": "^3.5.3",
    "typescript": "^5.8.3"
  }
}
